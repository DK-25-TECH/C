
#include <stdio.h>
#include<string.h>
int main()
{
    char c[200];
    fgets(c,sizeof(c),stdin);
    int n=strlen(c);
    if(c[n-1]=='\n')
    {
        c[n-1]='\0';
        n--;
    }
       int x=1;
    for(int i=0;i<n;i++)
    {
        if(c[i]==c[i+1])
        {
            x++;
        }
        else
        {
            printf("%c %d",c[i],x);
            x=1;
        }
    }
    
       
}
