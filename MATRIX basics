
#include <stdio.h>
int main()
{
    int r,c;
    scanf("%d %d",&r,&c);
    int a[r][c];
    int b[r][c];
    int add[r][c];
    int sub[r][c];
    int mul[r][c];
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            scanf("%d",&b[i][j]);
        }
    }
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            add[i][j]=a[i][j]+b[i][j];
            sub[i][j]=a[i][j]-b[i][j];
            mul[i][j]=a[i][j]*b[i][j];
        }
    }
    
    printf("ADDITION\n");
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
           printf("%d ",add[i][j]);
        }
        printf("\n");
    }
    printf("\n");
    printf("MULTI\n");
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
           printf("%d ",mul[i][j]);
        }
        printf("\n");
    }
    printf("\n");
    printf("SUBTRACTION\n");
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
           printf("%d ",sub[i][j]);
        }
        printf("\n");
    }
    printf("\n");
}
