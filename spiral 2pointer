#include<stdio.h>
#include<string.h>
#include<ctype.h>
#include<stdlib.h>
int main()
{
    int r=3,c=3;
    int **a=malloc(r*sizeof(int *));
    for(int i=0;i<r;i++)
    {
        *(a+i)=malloc(c*sizeof(int));
    }
    // int **b=malloc(r*sizeof(int *));
    // for(int i=0;i<r;i++)
    // {
    //     *(b+i)=malloc(c*sizeof(int));
    // }
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            scanf("%d",&(*(*(a+i)+j)));
        }
    }
   int lft=0,ryt=c-1,tp=0,bot=r-1;
    
    while(tp<=bot && lft<=ryt)
    {
        for(int i=lft;i<=ryt;i++)
        {
            printf("%d ",*(*(a+tp)+i));
        }
        tp++;
        for(int i=tp;i<=bot;i++)
        {
            printf("%d ",*(*(a+i)+ryt));
        }
        ryt--;
        
        if(tp<=bot)
        {
            for(int i=ryt;i>=lft;i--)
            {
                printf("%d ",*(*(a+bot)+i));
            }
            bot--;
        }
        if(lft<=ryt)
        {
            for(int i=bot;i>=tp;i--)
            {
                printf("%d ",*(*(a+i)+lft));
            }
            lft++;
        }
    }
}















