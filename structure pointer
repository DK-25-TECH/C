#include <stdio.h>
#include<stdlib.h>
struct student{
    int id;
    char name[20];
};
void insert(struct student *s,int n)
{
    for(int i=0;i<n;i++)
    {
        scanf("%d",&s[i].id);
        scanf("%19s",s[i].name);
    }
}
void display(struct student *s,int n)
{
    for(int i=0;i<n;i++)
    {
        printf("%d ",s[i].id);
        printf("%s ",s[i].name);
        printf("\n");
    }
}
int main() {
    int n;
    scanf("%d",&n);
    struct student *s=malloc(n*sizeof(struct student));
    if(s==NULL)
    {
        printf("no mempory");
    }
    insert(s,n);
    display(s,n);
}
