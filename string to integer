#include<stdio.h>
#include<string.h>
#include<ctype.h>
int main()
{
    char c[100];
    scanf("%s",c);
    int n=strlen(c);
    if(c[n-1]=='\n')
    {
        c[n-1]='\0';
        n--;
    }
    int x=0;
    for(int i=0;i<n;i++)
    {
        if(isdigit(c[i]))
        {
            x+=c[i]-'0';
        }
    }
    printf("%d",x);
}
