
#include <stdio.h>
#include<string.h>
int main()
{
    char c[100];
    fgets(c,sizeof(c),stdin);
    int n=strlen(c);
    if(c[n-1]=='\n')
    {
        c[n-1]='\0';
        n--;
    }
    char x[100];
    int t=-1;
    int z=1;
    for(int i=0;i<n;i++)
    {
        if(c[i]=='(' || c[i]=='[' || c[i]=='{')
        {
            x[++t]=c[i];
        }
        else if(c[i]==')' || c[i]==']' || c[i]=='}')
        {
            if(t==-1)
            {
                z=0;
                break;
            }
        else
        {
            if(c[i]=='}' && x[t]!='{' || c[i]==']' && x[t]!='[' || c[i]==')' && x[t]!='(')
            {
                z=0;
                break;
            }
            t--;
        }
        }
    }
        if(z && t==-1)
        {
            printf("yes");
        }
        else
        {
            printf("no");
        }
}
