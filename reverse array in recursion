#include<stdio.h>

void swap(int l,int r,int a[])
{
    if(l<r)
    {
        int z=a[l];
        a[l]=a[r];
        a[r]=z;
       
       swap(l+1,r-1,a);
    }
}
int main()
{
   int x;
    scanf("%d",&x);
    int a[x];
    for(int i=0;i<x;i++)
    {
        scanf("%d",&a[i]);
    }
    int l=0,r=x-1;
    swap(l,r,a);
    for(int i=0;i<x;i++)
    {
        printf("%d",a[i]);
    }
    return 0;
}
