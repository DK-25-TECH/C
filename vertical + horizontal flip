#include <stdio.h>

int main() {
    int r, c;

    scanf("%d %d", &r, &c);

    int a[r][c];
  
    for (int i = 0; i < r; i++)
        for (int j = 0; j < c; j++)
            scanf("%d", &a[i][j]);

    for (int i = 0; i < r / 2; i++) {
        for (int j = 0; j < c; j++) {
            int temp = a[i][j];
            a[i][j] = a[r - i - 1][c - j - 1];
            a[r - i - 1][c - j - 1] = temp;
        }
    }

    if (r % 2 == 1) { 
        int i = r / 2;
        for (int j = 0; j < c / 2; j++) {
            int temp = a[i][j];
            a[i][j] = a[i][c - j - 1];
            a[i][c - j - 1] = temp;
        }
    }

    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++)
            printf("%d ", a[i][j]);
        printf("\n");
    }

    return 0;
}
