
#include <stdio.h>
#include<string.h>
#include<limits.h>
void convert(char *c,int n)
{
    while(*c){
    if(*c>=97 && *c<=122)
    {
        *c-=32;
    }
    else if(*c>=65 && *c<=90)
    {
        *c+=32;
    }
    c++;
    }
}
int main() {
   char c[100];
   fgets(c,sizeof(c),stdin);
   int n=strlen(c);
   if(c[n-1]=='\n')
   {
       c[n-1]='\0';
       n--;
   }
   convert(c,n);
   printf("%s",c);
}
