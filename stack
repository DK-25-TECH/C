
#include <stdio.h>
int st[100];
int t=-1;
void push(int data)
{
    int n=sizeof(st)/sizeof(st[0]);
    if(t==n-1)
    {
        printf("STACK IS FULL");
        return;
    }
  
    st[++t]=data;
}
void pop()
{
    if(t==-1)
    {
        printf("STACK IS EMPTY");
        return;
    }
    --t;
}
void display()
{
    if(t==-1)
    {
        printf("STACK IS EMPTY");
        return;
    }
    for(int i=0;i<=t;i++)
    {
        printf("%d ",st[i]);
    }
}
int main()
{
    push(10);
    push(20);
    push(30);
    display();
    printf("\n");
    pop();
    pop();
    pop();
    display();
    
}
