#include<stdio.h>
#include<string.h>
int swap(int i,int n,char a[])
{
   
    if(i>=n/2)
    {
        return 1;
    }
        if(a[i]!=a[n-i-1])
        {
            return 0;
        }
        return swap(i+1,n,a);
}
int main()
{
    char a[100];
    fgets(a,sizeof(a),stdin);
    int n=strlen(a)-1;
    if(a[n-1]=='\n')
    {
        a[n-1]='\0';
        n--;
    }
   int x=swap(0,n,a);
    if(x)
    {
    printf("yes");
    }
    else
    {
    printf("no");
        
    }
    return 0;
}
