#include<stdio.h>
#include<string.h>
void lower(char *c,int n)
{
    for(int i=0;i<n;i++)
    {
        if(*(c+i)>='A' && *(c+i)<='Z')
        {
            *(c+i) +=32;
        }
    }
}
void upper(char *c,int n)
{
    for(int i=0;i<n;i++)
    {
        if(*(c+i)>='a' && *(c+i)<='z')
        {
            *(c+i) -=32;
        }
    }
}
int main() {
    char c[100];
    fgets(c,sizeof(c),stdin);
    int n=strlen(c);
    if(c[n-1]=='\n')
    {
        c[n-1]='\0';
    }
    lower(c,n);
    printf("%s\n",c);
    upper(c,n);
    printf("%s",c);
}
