#include<stdio.h>
void swap(int *l,int *r)
{
     while(l<r)
     {
         int x=*l;
         *l=*r;
         *r=x;
         l++;
         r--;
     }
    
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
   
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    
    int *l=a;
    int *r=l+n-1;
    swap(l,r);
    for(int i=0;i<n;i++)
    {
        printf("%d",a[i]);
    }
    return 0;
}
