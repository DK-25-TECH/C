
#include <stdio.h>
#include<string.h>
void cpy(char *a,char *b)
{
   while(*a)
   {
       *b++=*a++;
   }
  
   
   
}

int main()
{
    char a[100];
    char b[100];
    fgets(a,sizeof(a),stdin);
    if(a[strlen(a)-1]=='\n')
    {
        a[strlen(a)-1]='\0';
    }
    cpy(a,b);
    printf("%s ",b);
}
