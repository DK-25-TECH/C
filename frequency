#include<stdio.h>
#include<string.h>
int main()
{
    char c[100];
    int f[256]={0};
    fgets(c,sizeof(c),stdin);
    for(int i=0;c[i]!=0;i++)
    {
        f[(unsigned char)c[i]]++;
    }
    for(int i=0;i<256;i++)
    {
        if(f[i]!=0 && i!='\n')
        {
            printf("%c - %d\n",i,f[i]);
        }
    }
}
