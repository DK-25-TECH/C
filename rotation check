#include <stdio.h>
#include<string.h>
int check(char *a,char *b,int n1,int n2)
{
    for(int i=0;i<n2;i++)
    {
        int x=1;
        for(int j=0;j<n1;j++)
        {
            if(a[j]!=b[(i+j)%n2])
            {
               x=0;
               break;
            }
        }
        if(x)
        {
            return 1;
        }
    }
    return 0;
}
int main()
{
    char a[100];
    char b[100];
    fgets(a,sizeof(a),stdin);
    fgets(b,sizeof(b),stdin);
    int n1=strlen(a);
    int n2=strlen(b);
    if(a[n1-1]=='\n')
    {
        a[n1-1]='\0';
        n1--;
    }
    if(b[n2-1]=='\n')
    {
        b[n2-1]='\0';
        n2--;
    }
    int x=check(a,b,n1,n2);
    if(x)
    {
        printf("s");
    }
    else
    {
        printf("X");
    }

    return 0;
}
