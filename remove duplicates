#include <stdio.h>
#include<string.h>
#include<ctype.h>
#include<stdlib.h>
int removee(char *c,int n,int x[])
{
    int j=0;
    for(int i=0;i<n;i++)
    {
        if(x[(unsigned char)c[i]]==1)
        {
            c[j++]=c[i];
        }
    }
    return j;
}
int main()
{
   char c[100];
   fgets(c,sizeof(c),stdin);
   int n=strlen(c)-1;
   if(c[n]=='\n')
   {
       c[n]='\0';
       n--;
   }
   int x[256]={0};
   for(int i=0;i<n;i++)
   {
       x[(unsigned char)c[i]]++;
   }
   int j=removee(c,n,x);
   for(int i=0;i<j;i++)
   {
       printf("%c",c[i]);
   }
}
