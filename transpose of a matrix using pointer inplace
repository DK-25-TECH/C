#include<stdio.h>
#include<stdlib.h>
int main()
{
    int r,c;
    scanf("%d %d",&r,&c);
    int *a=malloc(r*c*sizeof(int));
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            scanf("%d",(a+i*c+j));
        }
    }
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            printf("%d ",*(a+i*c+j));
        }
        printf("\n");
    }
        printf("\n");
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            printf("%d ",*(a+j*c+i));
        }
        printf("\n");
    }
    free(a);
}










