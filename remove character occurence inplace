#include<stdio.h>
#include<string.h>
#include<ctype.h>
int main()
{
    char c[100];
    int frq[250]={0};
    fgets(c,sizeof(c),stdin);
    
    char x;
    int j=0;
    scanf("%c",&x);
    for(int i=0;i<strlen(c)-1;i++)
    {
       if(c[i]!=x && c[i]!='\n')
       {
           c[j++]=c[i];
       }
    }
    c[j+1]='\0';
    for(int i=0;i<strlen(c)-1;i++)
    {
      printf("%c",c[i]);
    }    
      
}
