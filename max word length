#include<stdio.h>
#include<string.h>
#include<ctype.h>
int main() {
    char a[100];
    fgets(a,sizeof(a),stdin);
    int n=strlen(a);
    if(a[n-1]=='\n')
    {
        a[n-1]='\0';
        n--;
    }
    int ml=0,start=0,end=0,ws=0;
    for(int i=0;i<=n;i++)
    {
        if(a[i]==' ' || a[i]=='\0' || a[i]=='\n')
        {
            int wl=i-start;
            if(wl>ml)
            {
                ml=wl;
                ws=start;
            }
            start=i+1;
        }
    }
    for(int i=ws;i<=ws+ml;i++)
    {
        printf("%c",a[i]);
    }
    
        // printf("%d",n);
   
    
}
