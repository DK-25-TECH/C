#include<stdio.h>
#include<stdlib.h>
void prime(int *x,int n)
{
    for(int i=0;i<n;i++)
    {
        int z=*(x+i);
        int flag=1;
        if(z<2)
        {
            flag=0;
        }
        else{
        for(int j=2;j<z;j++)
        {
            if(z%j==0)
            {
                flag=0;
                break;
            }
        }
        }
        if(flag)
        {
            printf("%d ",*(x+i));
        }
    }
}
int main()
{
    int n;
    scanf("%d",&n);
    int *x=malloc(n*sizeof(int));
    for(int i=0;i<n;i++)
    {
        *(x+i)=i+1;
    }
    prime(x,n);
    free(x);
}
