#include<stdio.h>
int main()
{
    char a[100];
    int frq[256]={0};
    int x[256]={0};
    int n;
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        scanf(" %c",&a[i]);
    }
    for(int i=0;i<n;i++)
    {
        frq[(unsigned char)a[i]]++;
    }
    for(int i=0;i<n;i++)
    {
        unsigned char ch=(unsigned char)a[i];
        if(!x[ch])
        {
              if(frq[ch]>1)
               {
                     printf("%c %d\n",a[i],frq[ch]);
               }
                x[ch]=1;
        }
    }
}
