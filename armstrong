#include<stdio.h>
#include<stdlib.h>
#include<math.h>
int amstrong(int n)
{
    int x=0,val=n,c=0;
    while(val>0)
    {
        c++;
        val/=10;
    }
    int tp=n;
    while(n>0)
    {
        int rem=n%10;
        x+=pow(rem,c);
        n/=10;
    }
    if(x==tp)
    {
        return 1;
    }
    return 0;
}
    
int main()
{
    int n;
    scanf("%d",&n);
    int x=amstrong(n);
    if(x)
    {
        printf("yes");
    }
    else{
         printf("no");
    }
}
