#include<stdio.h>
#include<stdlib.h>
#include<string.h>
void ans(char a[],int n)
{
    printf("%c",a[0]-32);
    for(int i=0;i<n;i++)
    {
        if(a[i]==' ' && a[i+1]!=' ' && a[i+1]!='\0')
        {
            printf("%c",a[i+1]-32);
        }
    }
}
int main()
{
    char a[100];
    fgets(a,sizeof(a),stdin);
    int n=strlen(a)-1;
    if(a[n]=='\n')
    {
        a[n]=='\0';
        n--;
    }
    ans(a,n);
}
