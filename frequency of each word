
#include<stdio.h>
#include<string.h>
#include<stdlib.h>
int main()
{
    char a[200];
    fgets(a,sizeof(a),stdin);
    int n=strlen(a);
    if(a[n-1]=='\n')
    {
        a[n-1]='\0';
        n--;
    }
    char *x=strtok(a," ");
    char cc[100][100];
    int f[50]={0};
    int c=0;
    while(x!=NULL)
    {
       int ff=0;
       for(int i=0;i<c;i++)
       {
           if(strcmp(cc[i],x)==0)
           {
               f[i]++;
                ff=1;
                break;
           }
       }
       if(!ff)
       {
           strcpy(cc[c],x);
           f[c]=1;
           c++;
       }
       x=strtok(NULL," ");
    }
    for(int i=0;i<c;i++)
    {
        printf("%s %d\n",cc[i],f[i]);
    }
    
    
}
