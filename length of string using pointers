
#include <stdio.h>
#include<string.h>
int cpy(char *a)
{
    int x=0;
   while(*a)
   {
       x++;
       a++;
   }
  
   return x;
   
}

int main()
{
    char a[100];
    
    fgets(a,sizeof(a),stdin);
    if(a[strlen(a)-1]=='\n')
    {
        a[strlen(a)-1]='\0';
    }
    int x=cpy(a);
    printf("%d ",x);
}
