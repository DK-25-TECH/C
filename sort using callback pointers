
#include <stdio.h>
#include<string.h>
int func(int a,int b)
{
    return a>b;
}
void sort(int *a,int n,int (*xx)(int,int))
{
    for(int i=0;i<n-1;i++)
    {
        for(int j=0;j<n-i-1;j++)
        {
            if(xx(a[j],a[j+1]))
            {
              int x=a[j];
              a[j]=a[j+1];
              a[j+1]=x;
            }
        }
    }
   
}

int main()
{
   int n;
   scanf("%d",&n);
   int a[n];
   for(int i=0;i<n;i++)
   {
       scanf("%d",&a[i]);
   }
   sort(a,n,func);
   for(int i=0;i<n;i++)
   {
       printf("%d ",a[i]);
   }
}
