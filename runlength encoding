
#include <stdio.h>
#include<string.h>
int main()
{
    char c[20];
    
    scanf("%s",c);
    for(int i=0;i<strlen(c)-1;i++)
    {
        int x=1;
        while(i<=strlen(c)-1 && c[i]==c[i+1])
        {
            x++;
            i++;
        }
        printf("%c->%d",c[i],x);
        
    }
}
