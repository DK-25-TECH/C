#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    int x[n];
    int mx=0; 
    for(int i=0;i<n;i++)
    {
        x[i]=1;
    }
    
    for(int i=1;i<n;i++)
    {
        for(int j=0;j<i;j++)
        {
            if(a[i]>a[j] && x[i]<x[j]+1)
            {
                x[i]=x[j]+1;
            }
        }
        if(mx<x[i])
        {
            mx=x[i];
        }
    }
    printf("%d",mx);
    
}
