#include <stdio.h>
int main()
{
    int r,c;
    scanf("%d %d",&r,&c);
    int a[r][c];
    int b[r][c];
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    for(int i=0;i<r;i++)
    {
        int x=a[i][0];
        for(int j=0;j<c;j++)
        {
            if(x<a[i][j])
            {
                x=a[i][j];
            }
        }
        printf("%d ",x);
    }
    printf("\n");
        for(int j=0;j<c;j++)
        {
              int x=a[0][j];
        for(int i=0;i<r;i++)
        {
            if(x<a[i][j])
            {
                x=a[i][j];
            }
        }
        printf("%d ",x);
    }
}
