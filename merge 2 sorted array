#include<stdio.h>
#include<stdbool.h>
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    int b[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        scanf("%d",&b[i]);
    }
    int x[n+n];
    int z=0;
    for(int i=0;i<n;i++)
    {
        x[z++]=a[i];
    }
    for(int i=0;i<n;i++)
    {
        x[z++]=b[i];
    }
    for(int i=0;i<z;i++)
    {
        for(int j=0;j<z-i-1;j++)
        {
            if(x[j]>x[j+1])
            {
                int xx=x[j];
                x[j]=x[j+1];
                x[j+1]=xx;
            }
        }
    }
    for(int i=0;i<n*2;i++)
    {
        printf("%d ",x[i]);
    }
   
}
