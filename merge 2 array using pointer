
#include<stdio.h>
#include<stdlib.h>
void sort(int *a,int *b,int n,int m,int *p)
{
    for(int i=0;i<n;i++)
    {
        *(p++)=*(a++);
    }
    for(int i=0;i<m;i++)
    {
        *(p++)=*(b++);
    }
}

int main()
{
   int n,m;
   scanf("%d",&n);
   int a[n];
   for(int i=0;i<n;i++)
   {
       scanf("%d",&a[i]);
   }
   scanf("%d",&m);
   int b[m];
   for(int i=0;i<m;i++)
   {
       scanf("%d",&b[i]);
   }
   int *p=malloc((n+m)*sizeof(int));
   sort(a,b,n,m,p);
   for(int i=0;i<n+m;i++)
   {
       printf("%d ",p[i]);
   }
}
