#include<stdio.h>
#include<stdlib.h>
#include<string.h>

int main()
{
    char c[100];
    char x[100];
    fgets(c,sizeof(c),stdin);
   
    int n=strlen(c);
    if(c[n-1]=='\n')
    {
        c[n-1]='\0';
    }
  
    int f[256]={0};
    for(int i=0;i<n;i++)
    {
            f[(unsigned char)c[i]]++;
    }
    for(int i=0;i<256;i++)
    {
            if(f[i]>0)
            {
                printf("%c%d",(char)i,f[i]);
            }
    }
}
