#include<stdio.h>
#include<string.h>
#include<ctype.h>
int isprime(int n)
{
    if(n<2)
    {
        return 0;
    }
    for(int i=2;i*i<=n;i++)
    {
        if(n%i==0)
        {
            return 0;
        }
    }
    return 1;
}
int main()
{
    char c[200];
    fgets(c,sizeof(c),stdin);
    int n=strlen(c);
    if(c[n-1]=='\n')
    {
        c[n-1]='\0';
        n--;
    }
    for(int i=0;i<n;i++)
    {
        if(isprime(i) && islower(c[i]))
        {
            c[i]-=32;
        }
    }
    for(int i=0;i<n;i++)
    {
        printf("%c",c[i]);
    }
}
