
#include<stdio.h>
int add(int a,int b)
{
    return a+b;
}
int sub(int a,int b)
{
    return a-b;
}
int mul(int a,int b)
{
    return a*b;
}
int divv(int a,int b)
{
    return a/b;
}

int main()
{
    int (*fp[])(int,int)={add,sub,mul,divv};
    int ch,a,b;
    scanf("%d",&ch);
    scanf("%d",&a);
    scanf("%d",&b);
    int x=fp[ch](a,b);
    printf("%d",x);
}
