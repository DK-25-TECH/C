#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
   int rr[n];
   int lr[n];
   int k;
   scanf("%d",&k);
   k%=n;
   for(int i=0;i<n;i++)
   {
       lr[i]=a[(i+k)%n];
       rr[(i+k)%n]=a[i];
   }
   for(int i=0;i<n;i++)
   {
       printf("%d ",lr[i]);
   }
   printf("\n");
   for(int i=0;i<n;i++)
   {
       printf("%d ",rr[i]);
       
   }
}

    
