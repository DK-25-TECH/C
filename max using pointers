#include<stdio.h>
#include<stdlib.h>
int findmax(int *p,int n)
{
    int x=*p;
    for(int i=0;i<n;i++)
    {
        if(*(p+i)>x)
        {
            x=*(p+i);
        }
    }
    return x;
}
int main()
{
    int n;
    scanf("%d",&n);
    int *p=malloc(n*sizeof(int));
    for(int i=0;i<n;i++)
    {
        scanf("%d",(p+i));
    }
    int x=findmax(p,n);
    
        printf("%d",x);
   
}
