#include<stdio.h>
void subset(int i,int a[],int b[],int n,int bl)
{
    if(i==n)
    {
        printf("{");
        for(int i=0;i<bl;i++)
        {
            printf("%d ",b[i]);
        }
        printf("}");
        return;
    }
    b[bl]=a[i];
    subset(i+1,a,b,n,bl+1);
    subset(i+1,a,b,n,bl);
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    int b[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    subset(0,a,b,n,0);
    return 0;
}
