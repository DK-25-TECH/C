#include<stdio.h>
void swap(int *l,int *r,int n)
{
     for(int i=0;i<n;i++)
     {
         int x=*l;
         *l=*r;
         *r=x;
         l++;
         r++;
     }
    
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    int b[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++)
    {
        scanf("%d",&b[i]);
    }
    int *l=a;
    int *r=b;
    swap(l,r,n);
    for(int i=0;i<n;i++)
    {
        printf("%d",a[i]);
    }
    printf("\n");
    for(int i=0;i<n;i++)
    {
        printf("%d",b[i]);
    }
    return 0;
}
