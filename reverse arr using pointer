
#include <stdio.h>

void rev(int *p,int n)
{
   
   for(int i=0;i<n/2;i++)
   {
       int x=*(p+i);
       *(p+i)=*(p+n-i-1);
       *(p+n-i-1)=x;
   }
   
   
}

int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    rev(a,n);
   for(int i=0;i<n;i++)
    printf("%d ",a[i]);
}
