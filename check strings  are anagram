#include<stdio.h>
#include<string.h>
#include<ctype.h>
void lower(char *s)
{
    while(*s)
    {
        *s=tolower(*s);
        s++;
    }
}
int isanagram(char *a,char *b,int n,int n1)
{
    if(n!=n1)
    {
        return 0;
    }
    lower(a);
    lower(b);
    int x[256]={0};
    for(int i=0;i<n;i++)
    {
        x[(unsigned char)a[i]]++;
    }
    for(int i=0;i<n1;i++)
    {
        x[(unsigned char)b[i]]--;
    }
    for(int i=0;i<256;i++)
    {
        if(x[i]!=0)
        {
            return 0;
        }
    }
    return 1;
}
int main() {
    char a[100];
    fgets(a,sizeof(a),stdin);
    int n=strlen(a);
    if(a[n-1]=='\n')
    {
        a[n-1]='\0';
    }
    char b[100];
    fgets(b,sizeof(b),stdin);
    int n1=strlen(b);
    if(b[n1-1]=='\n')
    {
        b[n1-1]='\0';
    }
    int x=isanagram(a,b,n,n1);
    if(x)
    {
        printf("true");
    }
    else
    {
        printf("false");
    }
}
