#include <stdio.h>
#include<string.h>
#include<ctype.h>
#include<stdlib.h>
int main()
{
   char c[100];
   fgets(c,sizeof(c),stdin);
   int n=strlen(c)-1;
   if(c[n]=='\n')
   {
       c[n]='\0';
       n--;
   }
   int x[256]={0};
   for(int i=0;i<n;i++)
   {
       x[(unsigned char)c[i]]++;
   }
   int mx=0;
   char xx;
   for(int i=0;i<n;i++)
   {
       if(x[(unsigned char)c[i]]>mx)
       {
           mx=x[(unsigned char)c[i]];
           xx=c[i];
       }
   }
           printf("%c -> %d\n",xx,mx);
}
