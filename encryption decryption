
#include <stdio.h>
#include<string.h>
void encrypt(char c[],int n,int x)
{
    for(int i=0;i<n;i++)
    {
        c[i]-=x;
    }
}
void decrypt(char c[],int n,int x)
{
    for(int i=0;i<n;i++)
    {
        c[i]+=x;
    }
}
int main()
{
    char c[200];
    fgets(c,sizeof(c),stdin);
    int n=strlen(c);
    if(c[n-1]=='\n')
    {
        c[n-1]='\0';
        n--;
    }
       
    int x;
    scanf("%d",&x);
    
    encrypt(c,n,x);
    
    for(int i=0;i<n;i++)
    {
        
       printf("%c ",c[i]);  
        
    }
    printf("\n");
    decrypt(c,n,x);
    for(int i=0;i<n;i++)
    {
        
       printf("%c ",c[i]);  
        
    }
}
