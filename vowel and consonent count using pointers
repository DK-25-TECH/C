#include<stdio.h>
#include<string.h>
#include<ctype.h>
void count(char *x,int *cc,int *v)
{
    for(int i=0;x[i]!='\0';i++)
    {
        char c=tolower(x[i]);
        if(c=='a' || c=='e' || c=='i' || c=='o' || c=='u')
        {
            (*cc)++;
            
        }
        else{
            (*v)++;
        }
    }
}
int main()
{
    char x[100];
    fgets(x,sizeof(x),stdin);
    int cc=0,v=0;
    count(x,&cc,&v);
    printf("%d %d",cc,v);
      
}
