#include<stdio.h>
#include<string.h>
int main()
{
    char c[100];
    scanf("%s",c);
    int n=strlen(c);
    if(c[n-1]=='\n')
    {
        c[n-1]='\0';
        n--;
    }
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n-i-1;j++)
        {
            if(c[j]>c[j+1])
            {
                char x=c[j];
                c[j]=c[j+1];
                c[j+1]=x;
            }
        }
    }
    printf("%s",c);
}
