#include <stdio.h>
#include<string.h>
#include<ctype.h>

int find(int b,char *x,char *c)
{
      while(*c)
      {
          if(strncmp(c,x,b)==0)
          
              return 1;
          
          c++;
      }
      return 0;
}
int main()
{
   int n;
   scanf("%d",&n);
   char c[100];
   fgets(c,sizeof(c),stdin);
   char x[100];
   fgets(x,sizeof(x),stdin);
   int a=strlen(c)-1;
   int b=strlen(x)-1;
   if(find(b,x,c))
   {
       printf("yes");
   }
   else
   {
       printf("no");
   }
}
