
#include <stdio.h>

int max(int *p,int n)
{
   int x=*p;
   for(int i=1;i<n;i++)
   {
       if(x>*(p+i))
       {
           x=*(p+i);
       }
   }
   
   return x;
}
int min(int *p,int n)
{
   int x=*p;
   for(int i=1;i<n;i++)
   {
       if(x<*(p+i))
       {
           x=*(p+i);
       }
   }
   
   return x;
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    int maxx=max(a,n);
    int minn=min(a,n);
    printf("%d\n",maxx);
    printf("%d",minn);
}
