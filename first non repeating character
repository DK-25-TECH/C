#include<stdio.h>
#include<string.h>
#include<ctype.h>
int fnc(char *c ,int n)
{
    int x[256]={0};
    for(int i=0;i<n;i++)
    {
        x[(unsigned char)*(c+i)]++;
    }
    for(int i=0;i<n;i++)
    {
        if(x[(unsigned char)*(c+i)]==1)
        {
            return i;
        }
    }
    return -1;
}
int main()
{
    char c[100];
    fgets(c,sizeof(c),stdin);
    int x=fnc(c,strlen(c)-1);
    if(x!=-1)
    {
        printf("%c",c[x]);
    }
    else
    {
        
        printf("no such char");
    }
}
