// Find string length
// Copy string
// Concatenate strings
// Compare strings
// Reverse a string
// Convert to uppercase/lowercase
// Count vowels/consonants
// Check if palindrome
// Find a character in string
// Find a substring

#include<stdio.h>
#include<string.h>
#include<stdlib.h>
void sort(int *x,int z)
{
    for(int i=0;i<z;i++)
    {
        for(int j=0;j<z-i-1;j++)
        {
            if(*(x+j)>*(x+j+1))
            {
                int q=*(x+j);
                *(x+j)=*(x+j+1);
                *(x+j+1)=q;
            }
        }
    }
}
void merge(int a[],int b[],int *x,int z,int n,void (*fp)(int *,int))
{
    int *s=x;
    for(int i=0;i<n;i++)
    {
        *x++=*(a+i);
    }
    for(int i=0;i<n;i++)
    {
        *x++=*(b+i);
    }
    fp(s,z);
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    int b[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++)
    {
        scanf("%d",&b[i]);
    }
    int z=n+n;
    int *x=calloc(z,sizeof(int));
    merge(a,b,x,z,n,sort);
    for(int i=0;i<z;i++)
    {
        printf("%d ",x[i]);
    }
    
    
    
}
